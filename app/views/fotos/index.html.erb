<h1>Fotos</h1>

<table>
  <thead>
    <tr>
      <th>F subida</th>
      <th>Foto titulo</th>
      <th>Usuario</th>
      <th>Paciente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fotos.each do |foto| %>
      <tr>
        <td><%= foto.f_subida %></td>
        <td><%= foto.foto_titulo %></td>
        <td><%= foto.usuario %></td>
        <td><%= foto.paciente %></td>
        <td><%= link_to 'Ver', foto %></td>
        <td><%= link_to 'Editar', edit_foto_path(foto) %></td>
        <td><%= link_to 'Eliminar', foto, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nueva Foto', new_foto_path %>
