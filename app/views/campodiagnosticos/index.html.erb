<h1>Campodiagnosticos</h1>

<table>
  <thead>
    <tr>
      <th>Tipo grupo</th>
      <th>Nombre</th>
      <th>Valor</th>
      <th>Usuario</th>
      <th>Fotodiagnostico</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campodiagnosticos.each do |campodiagnostico| %>
      <tr>
        <td><%= campodiagnostico.tipo_grupo %></td>
        <td><%= campodiagnostico.nombre %></td>
        <td><%= campodiagnostico.valor %></td>
        <td><%= campodiagnostico.usuario %></td>
        <td><%= campodiagnostico.fotodiagnostico %></td>
        <td><%= link_to 'Ver', campodiagnostico %></td>
        <td><%= link_to 'Editar', edit_campodiagnostico_path(campodiagnostico) %></td>
        <td><%= link_to 'Eliminar', campodiagnostico, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Campodiagnostico', new_campodiagnostico_path %>
