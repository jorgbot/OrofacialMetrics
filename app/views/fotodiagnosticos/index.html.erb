<h1>Fotodiagnosticos</h1>

<table>
  <thead>
    <tr>
      <th>F evaluacion</th>
      <th>Id evaluacion</th>
      <th>Usuario</th>
      <th>Foto</th>
      <th>Paciente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fotodiagnosticos.each do |fotodiagnostico| %>
      <tr>
        <td><%= fotodiagnostico.f_evaluacion %></td>
        <td><%= fotodiagnostico.id_evaluacion %></td>
        <td><%= fotodiagnostico.usuario %></td>
        <td><%= fotodiagnostico.foto %></td>
        <td><%= fotodiagnostico.paciente %></td>
        <td><%= link_to 'Ver', fotodiagnostico %></td>
        <td><%= link_to 'Editar', edit_fotodiagnostico_path(fotodiagnostico) %></td>
        <td><%= link_to 'Eliminar', fotodiagnostico, method: :delete, data: { confirm: '¿Estás seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Fotodiagnostico', new_fotodiagnostico_path %>
